    "redis": "^4.6.10",
    "@types/redis": "^4.0.11",
    "typescript": "^5",
    "autoprefixer": "^10.4.21",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "postcss": "^8.5.6"
