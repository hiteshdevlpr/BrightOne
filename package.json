{
  "name": "brightone-ca",
  "version": "0.1.0",
  "private": true,
  "description": "Professional real estate photography, videography, 3D tours, and virtual staging services",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "eslint",
    "docker:build": "docker-compose build",
